<?php

use Drupal\weerblok\WeerblokService;
use GuzzleHttp\ClientInterface;

/**
 * Factory voor WeerblokService.
 */
function weerblok_getWeerblokService(): WeerblokService {
  $http_client = \Drupal::httpClient();
  $api_key = \Drupal::config('weerblok.settings')->get('api_key'); // of uit settings
  return new WeerblokService($http_client, $api_key);
}
